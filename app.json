{
  "settings": {
    "miniprogramRoot": "./",
    "minified": true,
    "enhance": true,
    "minSdkVersion": "2.29.2"
  },
  "pages": [
    "pages/index/index",
    "pages/share/share",
    "pages/background/background",
    "pages/profile/profile",
    "pages/profile/basic-settings/basic-settings",
    "pages/profile/card-settings/card-settings"
  ],
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#fff",
    "navigationBarTitleText": "时间牛马",
    "navigationBarTextStyle": "black",
    "navigationStyle": "custom"
  },
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#007AFF",
    "backgroundColor": "#ffffff",
    "borderStyle": "black",
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "首页",
        "iconPath": "assets/icons/home.png",
        "selectedIconPath": "assets/icons/home-active.png"
      },
      {
        "pagePath": "pages/share/share",
        "text": "分享",
        "iconPath": "assets/icons/share.png",
        "selectedIconPath": "assets/icons/share-active.png"
      },
      {
        "pagePath": "pages/background/background",
        "text": "背景",
        "iconPath": "assets/icons/theme.png",
        "selectedIconPath": "assets/icons/theme-active.png"
      },
      {
        "pagePath": "pages/profile/profile",
        "text": "我的",
        "iconPath": "assets/icons/profile.png",
        "selectedIconPath": "assets/icons/profile-active.png"
      }
    ]
  },
  "style": "v2",
  "renderer": "skyline",
  "rendererOptions": {
    "skyline": {
      "defaultDisplayBlock": true,
      "defaultContentBox": true,
      "tagNameStyleIsolation": "legacy",
      "disableABTest": true,
      "sdkVersionBegin": "3.0.0",
      "sdkVersionEnd": "15.255.255"
    }
  },
  "componentFramework": "glass-easel",
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents"
}